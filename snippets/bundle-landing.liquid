<script>
// IIFE - Immediately Invoked Function Expression
(function(yourcode) {

    // The global jQuery object is passed as a parameter
  	yourcode(window.jQuery, window, document);

  }(function($, window, document) {

    // The $ is now locally scoped

   // Listen for the jQuery ready event on the document
   $(function() {
     if ( $(window).width() > 667) {
    $(".ns-hero-image.desktop").each(function(){
      $(this).attr('src', $(this).attr('src').replace('_1x1.jpg', '.jpg'));
      $(this).attr('src', $(this).attr('src').replace('_1x1.png', '.png'));
    });
    }
    if ( $(window).width() < 667) {
   $(".ns-hero-image.mobile").each(function(){
     $(this).attr('src', $(this).attr('src').replace('_1x1.jpg', '_large.jpg'));
     $(this).attr('src', $(this).attr('src').replace('_1x1.png', '_large.png'));
   });
   }

     $("img.ns-img").each(function(){
       $(this).attr('src', $(this).attr('src').replace('_1x1.jpg', '_large.jpg'));
       $(this).attr('src', $(this).attr('src').replace('_1x1.png', '_large.png'));
     });

     //slick-intializer function
     if ( $(window).width() > 667) {
       $('.ns-link-holder').slick({
       infinite: true,
       speed: 1000,
       slidesToShow: 4,
       slidesToScroll: 4
       });
     }
     if ( $(window).width() < 667) {
       $('.ns-link-holder').slick({
       infinite: true,
       speed: 1000,
       slidesToShow: 2,
       slidesToScroll: 2
       });
     }

     //click events to use arrow images for slick
     $('.arrow-container-right').click(function(){
     //clicks the hidden slick button
     $(this).nextAll('.slick-slider').children('.slick-next').click();
     });

     $('.arrow-container-left').click(function(){
     //clicks the hidden slick button
     $(this).nextAll('.slick-slider').children('.slick-prev').click();
     });

     discountForm();
     function discountForm(){
       //for moving header "20$ off tag" and "Paypal Credit" tab
      if ($(window).width() > 667) {
        $('#twenty-off').appendTo('#site-header');
        $('#paypal-credit-tab').appendTo('#site-header');
      }
      else{
       $('#twenty-off').appendTo('#site-header-mobile');
       $('#paypal-credit-tab').appendTo('#site-header-mobile');

      }
      $('#bundle-overlay').appendTo('body');
      $('#twenty-off-form').appendTo('body');

      //slide functionality of perform
      $('#twenty-off span#click').click(function () {
        $('#bundle-overlay').css('display', 'block');
        $('#twenty-off-form').css('display', 'block').removeClass('slideOutLeft').addClass('animated slideInLeft');
        $('#bundle-overlay').click(function(){
          $('#twenty-off-form').removeClass('slideInLeft').addClass('slideOutLeft');
          $('#bundle-overlay').hide();
//           $('#twenty-off-form').css('display', 'none');
        });

      });
    };

    learnMorePop();

    function learnMorePop(){
      // functionality for learn more pop-up
      $('#learn-pop').click(function(){
        $('#ns-pop-up').appendTo('body');
        $('#ns-pop-up').css('display', 'block');
        $('#ns-close-pop-up').click(function(){
          $('#ns-pop-up').css('display', 'none');
        });
      });
    }

   })
 })
);

</script>
